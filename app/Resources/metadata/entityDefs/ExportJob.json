{
  "fields": {
    "name": {
      "type": "varchar",
      "required": true,
      "trim": true
    },
    "createdAt": {
      "type": "datetime",
      "readOnly": true
    },
    "modifiedAt": {
      "type": "datetime",
      "readOnly": true
    },
    "createdBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "modifiedBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "assignedUser": {
      "type": "link",
      "required": true,
      "view": "views/fields/assigned-user"
    },
    "teams": {
      "type": "linkMultiple",
      "view": "views/fields/teams"
    },
    "exportFeed": {
      "type": "link",
      "readOnly": true
    },
    "file": {
      "type": "file",
      "readOnly": true
    },
    "state": {
      "type": "enum",
      "required": true,
      "options": [
        "Pending",
        "Running",
        "Success",
        "Failed"
      ],
      "optionColors": [
        "00AFFF",
        "FFE208",
        "1DFF08",
        "FF0000"
      ],
      "default": "Pending",
      "readOnly": true,
      "view": "export:views/export-job/fields/state"
    },
    "stateMessage": {
      "type": "text",
      "readOnly": true
    },
    "start": {
      "type": "datetime",
      "readOnly": true
    },
    "end": {
      "type": "datetime",
      "readOnly": true
    },
    "sortOrder": {
      "type": "int"
    },
    "count": {
      "type": "int",
      "default": 0,
      "readOnly": true
    },
    "data": {
      "type": "jsonObject"
    }
  },
  "links": {
    "createdBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "modifiedBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "assignedUser": {
      "type": "belongsTo",
      "entity": "User"
    },
    "teams": {
      "type": "hasMany",
      "entity": "Team",
      "relationName": "EntityTeam",
      "layoutRelationshipsDisabled": true
    },
    "exportFeed": {
      "type": "belongsTo",
      "foreign": "exportJobs",
      "entity": "ExportFeed"
    },
    "file": {
      "type": "belongsTo",
      "entity": "Attachment",
      "skipOrmDefs": true
    }
  },
  "collection": {
    "sortBy": "sortOrder",
    "asc": false
  },
  "indexes": {
    "name": {
      "columns": [
        "name",
        "deleted"
      ]
    },
    "assignedUser": {
      "columns": [
        "assignedUserId",
        "deleted"
      ]
    }
  }
}