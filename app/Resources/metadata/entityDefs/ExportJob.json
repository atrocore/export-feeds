{
  "fields": {
    "name": {
      "type": "varchar",
      "required": true,
      "trim": true
    },
    "trial": {
      "type": "int",
      "default": 0
    },
    "createdAt": {
      "type": "datetime",
      "readOnly": true
    },
    "modifiedAt": {
      "type": "datetime",
      "readOnly": true
    },
    "createdBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "modifiedBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "exportFeed": {
      "type": "link",
      "readOnly": true
    },
    "file": {
      "type": "file",
      "readOnly": true
    },
    "state": {
      "type": "enum",
      "required": true,
      "options": [
        "Pending",
        "Running",
        "Success",
        "Failed",
        "Canceled"
      ],
      "optionColors": [
        "00AFFF",
        "FFE208",
        "1DFF08",
        "FF0000",
        "DDDDDD"
      ],
      "default": "Pending",
      "readOnly": true,
      "view": "export:views/export-job/fields/state"
    },
    "stateMessage": {
      "type": "text",
      "readOnly": true
    },
    "start": {
      "type": "datetime",
      "readOnly": true
    },
    "end": {
      "type": "datetime",
      "readOnly": true
    },
    "sortOrder": {
      "type": "int"
    },
    "count": {
      "type": "int",
      "default": 0,
      "readOnly": true
    },
    "data": {
      "type": "jsonObject"
    },
    "editable": {
      "type": "bool",
      "notStorable": true
    },
    "payload": {
      "type": "jsonObject",
      "notStorable": true,
      "layoutDetailDisabled": true,
      "layoutDetailSmallDisabled": true,
      "layoutListDisabled": true,
      "layoutListSmallDisabled": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true,
      "exportDisabled": true,
      "importDisabled": true,
      "textFilterDisabled": true,
      "emHidden": true
    }
  },
  "links": {
    "createdBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "modifiedBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "exportFeed": {
      "type": "belongsTo",
      "foreign": "exportJobs",
      "entity": "ExportFeed"
    },
    "file": {
      "type": "belongsTo",
      "entity": "Attachment",
      "skipOrmDefs": true
    }
  },
  "collection": {
    "sortBy": "sortOrder",
    "asc": false
  },
  "indexes": {
    "name": {
      "columns": [
        "name",
        "deleted"
      ]
    },
    "state": {
      "columns": [
        "state",
        "deleted"
      ]
    },
    "start": {
      "columns": [
        "start",
        "deleted"
      ]
    },
    "end": {
      "columns": [
        "end",
        "deleted"
      ]
    },
    "createdAt": {
      "columns": [
        "createdAt",
        "deleted"
      ]
    },
    "modifiedAt": {
      "columns": [
        "modifiedAt",
        "deleted"
      ]
    }
  }
}