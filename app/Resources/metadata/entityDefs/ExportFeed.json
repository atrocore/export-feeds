{
  "fields": {
    "name": {
      "type": "varchar",
      "required": true,
      "trim": true
    },
    "description": {
      "type": "text"
    },
    "limit": {
      "type": "int",
      "required": true,
      "default": 2000,
      "min": 1,
      "max": 10000
    },
    "separateJob": {
      "type": "bool"
    },
    "createdAt": {
      "type": "datetime",
      "readOnly": true
    },
    "modifiedAt": {
      "type": "datetime",
      "readOnly": true
    },
    "createdBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "modifiedBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "ownerUser": {
      "type": "link",
      "required": false,
      "view": "views/fields/owner-user"
    },
    "assignedUser": {
      "type": "link",
      "required": false,
      "view": "views/fields/assigned-user"
    },
    "teams": {
      "type": "linkMultiple",
      "view": "views/fields/teams"
    },
    "type": {
      "type": "enum",
      "required": true,
      "optionsIds": [
        "simple"
      ],
      "options": [
        "simple"
      ],
      "default": "simple",
      "prohibitedEmptyValue": true
    },
    "language": {
      "type": "varchar",
      "view": "export:views/export-feed/fields/language",
      "tooltip": true
    },
    "isActive": {
      "notNull": true,
      "type": "bool"
    },
    "fileType": {
      "type": "enum",
      "required": false,
      "optionsIds": [
        "csv",
        "xlsx",
        "json",
        "xml"
      ],
      "options": [
        "csv",
        "xlsx",
        "json",
        "xml"
      ],
      "prohibitedEmptyValue": false
    },
    "isFileHeaderRow": {
      "type": "bool",
      "notStorable": true,
      "dataField": true,
      "default": true
    },
    "csvFieldDelimiter": {
      "type": "enum",
      "optionsIds": [
        ";",
        ",",
        "\\t"
      ],
      "options": [
        ";",
        ",",
        "\\t"
      ],
      "default": ";",
      "notStorable": true,
      "dataField": true,
      "view": "export:views/export-feed/fields/field-delimiter",
      "prohibitedEmptyValue": true
    },
    "csvTextQualifier": {
      "type": "enum",
      "optionsIds": [
        "doubleQuote",
        "singleQuote"
      ],
      "options": [
        "doubleQuote",
        "singleQuote"
      ],
      "default": "doubleQuote",
      "notStorable": true,
      "dataField": true,
      "prohibitedEmptyValue": true
    },
    "entity": {
      "type": "enum",
      "options": [],
      "required": true,
      "notStorable": true,
      "dataField": true,
      "prohibitedEmptyValue": true,
      "view": "export:views/export-feed/fields/entity"
    },
    "convertCollectionToString": {
      "type": "bool",
      "notStorable": true,
      "default": true,
      "dataField": true,
      "tooltip": true
    },
    "delimiter": {
      "type": "varchar",
      "notStorable": true,
      "dataField": true,
      "required": false,
      "default": "~"
    },
    "replaceAttributeValues": {
      "type": "bool",
      "notStorable": true,
      "default": true,
      "dataField": true,
      "tooltip": true
    },
    "convertRelationsToString": {
      "type": "bool",
      "notStorable": true,
      "default": true,
      "dataField": true,
      "tooltip": true
    },
    "fieldDelimiterForRelation": {
      "type": "varchar",
      "notStorable": true,
      "dataField": true,
      "required": false,
      "default": "|"
    },
    "emptyValue": {
      "type": "varchar",
      "notStorable": true,
      "dataField": true,
      "required": false,
      "default": ""
    },
    "nullValue": {
      "type": "varchar",
      "notStorable": true,
      "dataField": true,
      "required": false,
      "default": "Null"
    },
    "markForNotLinkedAttribute": {
      "type": "varchar",
      "notStorable": true,
      "dataField": true,
      "required": false,
      "default": "--"
    },
    "decimalMark": {
      "type": "varchar",
      "notStorable": true,
      "dataField": true,
      "required": false,
      "default": ","
    },
    "thousandSeparator": {
      "type": "varchar",
      "notStorable": true,
      "dataField": true,
      "required": false
    },
    "data": {
      "type": "jsonObject",
      "layoutDetailDisabled": true,
      "layoutListDisabled": true,
      "layoutMassUpdateDisabled": true,
      "filterDisabled": true
    },
    "configuratorItems": {
      "type": "linkMultiple",
      "layoutDetailDisabled": true,
      "layoutMassUpdateDisabled": true,
      "noLoad": true
    },
    "exportJobs": {
      "type": "linkMultiple",
      "layoutDetailDisabled": true,
      "layoutMassUpdateDisabled": true,
      "noLoad": true,
      "importDisabled": true
    },
    "sortOrderField": {
      "type": "varchar",
      "required": false,
      "view": "export:views/export-feed/fields/sort-order-field"
    },
    "sortOrderDirection": {
      "type": "enum",
      "required": false,
      "options": [
        "ASC",
        "DESC"
      ],
      "optionsIds": [
        "1",
        "2"
      ]
    },
    "template": {
      "type": "text",
      "useDisabledTextareaInViewMode": true,
      "rows": 20,
      "required": false,
      "tooltip": true,
      "view": "export:views/export-feed/fields/template"
    }
  },
  "links": {
    "createdBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "modifiedBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "ownerUser": {
      "type": "belongsTo",
      "entity": "User"
    },
    "assignedUser": {
      "type": "belongsTo",
      "entity": "User"
    },
    "teams": {
      "type": "hasMany",
      "entity": "Team",
      "relationName": "EntityTeam",
      "layoutRelationshipsDisabled": true
    },
    "configuratorItems": {
      "type": "hasMany",
      "foreign": "exportFeed",
      "entity": "ExportConfiguratorItem",
      "audited": false
    },
    "exportJobs": {
      "type": "hasMany",
      "foreign": "exportFeed",
      "entity": "ExportJob"
    }
  },
  "collection": {
    "sortBy": "createdAt",
    "asc": false
  },
  "indexes": {
    "name": {
      "columns": [
        "name",
        "deleted"
      ]
    },
    "ownerUser": {
      "columns": [
        "ownerUserId",
        "deleted"
      ]
    },
    "assignedUser": {
      "columns": [
        "assignedUserId",
        "deleted"
      ]
    }
  }
}
