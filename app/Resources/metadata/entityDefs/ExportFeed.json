{
  "fields": {
    "name": {
      "type": "varchar",
      "required": true,
      "trim": true
    },
    "description": {
      "type": "text"
    },
    "createdAt": {
      "type": "datetime",
      "readOnly": true
    },
    "modifiedAt": {
      "type": "datetime",
      "readOnly": true
    },
    "createdBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "modifiedBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "ownerUser": {
      "type": "link",
      "required": true,
      "view": "views/fields/owner-user"
    },
    "assignedUser": {
      "type": "link",
      "required": true,
      "view": "views/fields/assigned-user"
    },
    "teams": {
      "type": "linkMultiple",
      "view": "views/fields/teams"
    },
    "type": {
      "type": "enum",
      "required": true,
      "options": [
        "simple"
      ],
      "default": "simple",
      "prohibitedEmptyValue": true
    },
    "isActive": {
      "notNull": true,
      "type": "bool"
    },
    "fileType": {
      "type": "enum",
      "required": false,
      "options": [],
      "view": "export:views/export-feed/fields/file-type",
      "prohibitedEmptyValue": true
    },
    "isFileHeaderRow": {
      "type": "bool",
      "default": "1"
    },
    "csvFieldDelimiter": {
      "type": "enum",
      "options": [
        ";",
        ",",
        "\\t",
        "|"
      ],
      "default": ";",
      "view": "export:views/export-feed/fields/field-delimiter",
      "prohibitedEmptyValue": true
    },
    "csvTextQualifier": {
      "type": "enum",
      "options": [
        "doubleQuote",
        "singleQuote"
      ],
      "default": "doubleQuote",
      "prohibitedEmptyValue": true
    },
    "channel": {
      "type": "link",
      "tooltip": true
    },
    "data": {
      "type": "jsonObject",
      "layoutDetailDisabled": true,
      "layoutListDisabled": true,
      "layoutMassUpdateDisabled": true,
      "layoutFiltersDisabled": true
    },
    "exportResults": {
      "type": "linkMultiple",
      "layoutDetailDisabled": true,
      "layoutMassUpdateDisabled": true,
      "noLoad": true,
      "importDisabled": true
    }
  },
  "links": {
    "createdBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "modifiedBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "ownerUser": {
      "type": "belongsTo",
      "entity": "User"
    },
    "assignedUser": {
      "type": "belongsTo",
      "entity": "User"
    },
    "teams": {
      "type": "hasMany",
      "entity": "Team",
      "relationName": "EntityTeam",
      "layoutRelationshipsDisabled": true
    },
    "channel": {
      "type": "belongsTo",
      "entity": "Channel"
    },
    "exportResults": {
      "type": "hasMany",
      "foreign": "exportFeed",
      "entity": "ExportResult"
    }
  },
  "collection": {
    "sortBy": "createdAt",
    "asc": false
  },
  "indexes": {
    "name": {
      "columns": [
        "name",
        "deleted"
      ]
    },
    "ownerUser": {
      "columns": [
        "ownerUserId",
        "deleted"
      ]
    },
    "assignedUser": {
      "columns": [
        "assignedUserId",
        "deleted"
      ]
    }
  }
}
