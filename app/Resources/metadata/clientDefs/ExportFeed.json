{
  "controller": "controllers/record",
  "boolFilterList": [
    "onlyMy"
  ],
  "navigateToEntityAfterQuickCreate": true,
  "disabledSelectList": true,
  "quickCreateOptions": {
    "fullFormDisabled": true
  },
  "iconClass": "fas fa-cloud-upload-alt",
  "views": {
    "detail": "export:views/export-feed/detail"
  },
  "recordViews": {
    "detail": "export:views/export-feed/record/detail"
  },
  "bottomPanels": {
    "detail": [
      {
        "name": "simpleTypeEntityFilter",
        "label": "entityFilter",
        "view": "export:views/export-feed/simple-type-components/record/panels/simple-type-entity-filter",
        "notRefreshable": true
      }
    ]
  },
  "optionalBottomPanels": {
    "configuratorItems": [
      {
        "field": "hasConfigurator",
        "value": true
      }
    ]
  },
  "relationshipPanels": {
    "configuratorItems": {
      "select": false,
      "dragDrop": {
        "isActive": true,
        "sortField": "sortOrder"
      },
      "sortBy": "sortOrder",
      "asc": true,
      "view": "export:views/export-feed/record/panels/configurator-items",
      "rowActionsView": "views/record/row-actions/edit-and-remove"
    },
    "exportJobs": {
      "create": false,
      "select": false,
      "rowActionsView": "export:views/export-job/record/row-actions/relationship",
      "view": "export:views/export-feed/record/panels/export-jobs",
      "layout": "listInExportFeed"
    }
  },
  "customEntities": {
    "Product": {
      "productCategories": {
        "entity": "Category"
      }
    }
  },
  "dynamicLogic": {
    "fields": {
      "type": {
        "readOnly": {
          "conditionGroup": [
            {
              "type": "isNotEmpty",
              "attribute": "id"
            }
          ]
        }
      },
      "convertCollectionToString": {
        "visible": {
          "conditionGroup": [
            {
              "type": "isTrue",
              "attribute": "hasConfigurator"
            }
          ]
        },
        "readOnly": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            }
          ]
        }
      },
      "convertRelationsToString": {
        "visible": {
          "conditionGroup": [
            {
              "type": "isTrue",
              "attribute": "hasConfigurator"
            }
          ]
        },
        "readOnly": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            }
          ]
        }
      },
      "fileType": {
        "visible": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            }
          ]
        }
      },
      "csvFieldDelimiter": {
        "visible": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            },
            {
              "type": "equals",
              "attribute": "fileType",
              "value": "csv"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            },
            {
              "type": "equals",
              "attribute": "fileType",
              "value": "csv"
            }
          ]
        }
      },
      "csvTextQualifier": {
        "visible": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            },
            {
              "type": "equals",
              "attribute": "fileType",
              "value": "csv"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            },
            {
              "type": "equals",
              "attribute": "fileType",
              "value": "csv"
            }
          ]
        }
      },
      "isFileHeaderRow": {
        "visible": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            },
            {
              "type": "in",
              "attribute": "fileType",
              "value": [
                "xlsx",
                "csv"
              ]
            }
          ]
        }
      },
      "fieldDelimiterForRelation": {
        "visible": {
          "conditionGroup": [
            {
              "type": "isTrue",
              "attribute": "hasConfigurator"
            },
            {
              "type": "isTrue",
              "attribute": "convertRelationsToString"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "isTrue",
              "attribute": "hasConfigurator"
            },
            {
              "type": "isTrue",
              "attribute": "convertRelationsToString"
            }
          ]
        }
      },
      "delimiter": {
        "visible": {
          "conditionGroup": [
            {
              "type": "isTrue",
              "attribute": "hasConfigurator"
            },
            {
              "type": "isTrue",
              "attribute": "convertCollectionToString"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "isTrue",
              "attribute": "hasConfigurator"
            },
            {
              "type": "isTrue",
              "attribute": "convertCollectionToString"
            }
          ]
        }
      },
      "nullValue": {
        "visible": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            }
          ]
        }
      },
      "emptyValue": {
        "visible": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            }
          ]
        }
      },
      "thousandSeparator": {
        "visible": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            }
          ]
        }
      },
      "decimalMark": {
        "visible": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            }
          ]
        }
      },
      "replaceAttributeValues": {
        "visible": {
          "conditionGroup": [
            {
              "type": "isTrue",
              "attribute": "hasConfigurator"
            },
            {
              "type": "equals",
              "attribute": "entity",
              "value": "Product"
            }
          ]
        }
      },
      "markForNotLinkedAttribute": {
        "visible": {
          "conditionGroup": [
            {
              "type": "isTrue",
              "attribute": "hasConfigurator"
            },
            {
              "type": "equals",
              "attribute": "entity",
              "value": "Product"
            }
          ]
        }
      },
      "channel": {
        "visible": {
          "conditionGroup": [
            {
              "type": "isTrue",
              "attribute": "hasConfigurator"
            },
            {
              "type": "equals",
              "attribute": "entity",
              "value": "Product"
            }
          ]
        }
      },
      "language": {
        "visible": {
          "conditionGroup": [
            {
              "type": "isTrue",
              "attribute": "hasConfigurator"
            }
          ]
        }
      }
    }
  }
}