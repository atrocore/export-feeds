{
  "controller": "controllers/record",
  "boolFilterList": [
    "onlyMy"
  ],
  "quickCreate": true,
  "navigateToEntityAfterQuickCreate": true,
  "disabledSelectList": true,
  "quickCreateOptions": {
    "fullFormDisabled": true
  },
  "iconClass": "fas fa-cloud-upload-alt",
  "views": {
    "list": "export:views/list",
    "detail": "export:views/detail"
  },
  "recordViews": {
    "detail": "export:views/export-feed/record/detail",
    "list": "export:views/export-feed/record/list"
  },
  "bottomPanels": {
    "detail": [
      {
        "name": "simpleTypeEntityFilter",
        "label": "entityFilter",
        "view": "export:views/export-feed/simple-type-components/record/panels/simple-type-entity-filter",
        "notRefreshable": true
      },
      {
        "name": "simpleTypeSettings",
        "label": "simpleTypeSettings",
        "view": "export:views/export-feed/record/panels/simple-type-settings",
        "notRefreshable": true
      }
    ]
  },
  "relationshipPanels": {
    "exportJobs": {
      "create": false,
      "select": false,
      "rowActionsView": "views/record/row-actions/relationship-remove-only",
      "view": "export:views/export-feed/record/panels/export-jobs",
      "layout": "listInExportFeed"
    }
  },
  "customEntities": {
    "Product": {
      "productCategories": {
        "entity": "Category"
      }
    }
  },
  "dynamicLogic": {
    "fields": {
      "type": {
        "readOnly": {
          "conditionGroup": [
            {
              "type": "isNotEmpty",
              "attribute": "id"
            }
          ]
        }
      },
      "fileType": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        }
      },
      "csvFieldDelimiter": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            },
            {
              "type": "equals",
              "attribute": "fileType",
              "value": "csv"
            }
          ]
        }
      },
      "valueDelimiter": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "fileType",
              "value": "csv"
            },
            {
              "type": "in",
              "attribute": "type",
              "value": [
                "simple"
              ]
            }
          ]
        }
      },
      "csvTextQualifier": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            },
            {
              "type": "equals",
              "attribute": "fileType",
              "value": "csv"
            }
          ]
        }
      },
      "isFileHeaderRow": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            },
            {
              "type": "in",
              "attribute": "fileType",
              "value": [
                "xlsx",
                "csv"
              ]
            }
          ]
        }
      }
    }
  }
}