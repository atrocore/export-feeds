<div class="edit-container record">
    <div class="row">
        <div class="cell col-sm-6">
            <label class="control-label">
                <span class="label-text">{{translate 'Attribute' scope='Global' category='labels'}}</span>
            </label>
            <div class="field" data-name="attribute">{{{attribute}}}</div>
        </div>
        <div class="cell col-sm-6">
            <label class="control-label">
                <span class="label-text">{{translate 'column' scope='ExportFeed' category='fields'}}</span>
            </label>
            <div class="field" data-name="columnType">{{{columnType}}}</div>
            <div class="field" data-name="column">{{{column}}}</div>
        </div>
    </div>
    <div class="row">
        <div class="cell col-sm-6">
            <label class="control-label">
                <span class="label-text">{{translate 'Language' scope='ExportFeed' category='labels'}}</span>
            </label>
            <div class="field" data-name="locale">{{{locale}}}</div>
        </div>
    </div>
</div>